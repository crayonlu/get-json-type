{
  "name": "@crayonlu/get-json-type",
  "version": "0.1.0",
  "description": "A CLI tool to convert JSON data into TypeScript type definitions",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "get-json-type": "./dist/cli.js"
  },
  "files": [
    "dist",
    "README.md",
    "README.en.md",
    "LICENSE"
  ],
  "keywords": [
    "typescript",
    "json",
    "type",
    "cli",
    "converter",
    "generator",
    "interface"
  ],
  "author": "crayonlu",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/crayonlu/get-json-type.git"
  },
  "bugs": {
    "url": "https://github.com/crayonlu/get-json-type/issues"
  },
  "homepage": "https://github.com/crayonlu/get-json-type#readme",
  "scripts": {
    "dev": "bun run bin/cli.ts",
    "build": "bun build src/index.ts --outdir=dist --target=node --format=esm && bun build bin/cli.ts --outfile=dist/cli.js --target=node --format=esm",
    "prepublishOnly": "bun test && bun run build",
    "start": "bun run dist/cli.js",
    "test": "bun test",
    "format:check": "prettier --check .",
    "format": "prettier . --write"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@types/pluralize": "^0.0.33"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "dependencies": {
    "@clack/prompts": "^0.11.0",
    "cac": "^6.7.14",
    "clipboardy": "^5.0.1",
    "ora": "^9.0.0",
    "picocolors": "^1.1.1",
    "pluralize": "^8.0.0",
    "prettier": "3.7.4"
  }
}
